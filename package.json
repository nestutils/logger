{
  "name": "@nestutils/logger",
  "version": "0.0.1-rc.8",
  "private": false,
  "license": "MIT",
  "description": "Logging Module For NestJS",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "build": "rimraf lib && tsc --project tsconfig.build.json",
    "format": "prettier **/**/*.ts --ignore-path ./.prettierignore --write && git status",
    "publish": "npm run build && npm publish --access=public"
  },
  "keywords": [
    "logger",
    "nest-logger",
    "winston-nestjs"
  ],
  "author": "Priyanshu Belwal",
  "dependencies": {
    "safe-stable-stringify": "^2.4.3"
  },
  "devDependencies": {
    "@nestjs/common": "^9.4.0",
    "@nestjs/core": "^9.4.0",
    "@nestjs/testing": "^9.4.0",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "jest": "^29.5.0",
    "prettier": "^2.8.7",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.0",
    "rxjs": "^7.8.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nestutils/logger"
  }
}
